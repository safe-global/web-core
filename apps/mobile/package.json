  },
  "scripts": {
    "start": "expo start",
    "start:android": "expo run:android",
    "start:ios": "expo run:ios",
    "storybook:metro": "STORYBOOK_ENABLED='true' expo start",
    "storybook:web": "STORYBOOK_ENABLED='true' STORYBOOK_WEB='true' storybook dev -p 6006",
    "storybook:ios": "STORYBOOK_ENABLED='true' expo run:ios --device",
    "storybook:android": "STORYBOOK_ENABLED='true' expo start --android",
    "reset-project": "node ./scripts/reset-project.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:snapshots": "jest -u",
    "lint": "tsc && npm run eslint",
    "eslint": "eslint src/* app/*",
    "eslint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "storybook-generate": "sb-rn-get-stories",
    "generate:icons": "node ./scripts/generateIconTypes.js",
    "prepare": "husky",
    "build-storybook": "STORYBOOK_WEB='true' storybook build",
    "e2e:metro-ios": "EXPO_PUBLIC_ENV=e2e NODE_ENV=test RN_SRC_EXT=e2e.ts,e2e.tsx expo run:ios",
    "e2e:metro-android": "EXPO_PUBLIC_ENV=e2e NODE_ENV=test RN_SRC_EXT=e2e.ts,e2e.tsx expo run:android",
    "e2e:run": "maestro test e2e",
    "eas-build-pre-install": "corepack enable && yarn set version 4",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
